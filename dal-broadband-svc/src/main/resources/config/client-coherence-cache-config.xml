<?xml version="1.0"?>

<cache-config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.oracle.com/coherence/coherence-cache-config"
	xsi:schemaLocation="http://xmlns.oracle.com/coherence/coherence-cache-config
   coherence-cache-config.xsd">
	<caching-scheme-mapping>
		<cache-mapping>
			<cache-name>BankHolidaysCache</cache-name>
			<scheme-name>BankHolidaysCacheScheme</scheme-name>
			<key-type>java.sql.Date</key-type>
			<value-type>com.vodafone.bankHolidays.pojo.BankHolidays
			</value-type>
		</cache-mapping>
	</caching-scheme-mapping>

	<caching-schemes>
		<remote-cache-scheme>
			<scheme-name>BankHolidaysCacheScheme</scheme-name>
			<service-name>DALCoherenceCustomerDatagrid:ExtendTcpProxyCustomer
			</service-name>
			<initiator-config>
				<tcp-initiator>
					<socket-provider>
						<ssl>
							<protocol>TLS</protocol>
							<trust-manager>
								<algorithm>SunX509</algorithm>
								<key-store>
									<url>file:/apps/sb/coh-cert/cohadmin.jks</url>
									<password>p@ssw0rd</password>
									<type>JKS</type>
								</key-store>
							</trust-manager>
							<socket-provider>tcp</socket-provider>
						</ssl>
					</socket-provider>
					<name-service-addresses>
						<!--name-service-addresses:address-provider-instance -->
						<address-provider>
							<class-name>com.vf.uk.dal.broadband.utils.CoherenceAddressProvider
							</class-name>
						</address-provider>
					</name-service-addresses>
				</tcp-initiator>
				<outgoing-message-handler>
					<request-timeout>15s</request-timeout>
				</outgoing-message-handler>
				<serializer>
					<instance>
						<class-name>com.tangosol.io.pof.ConfigurablePofContext
						</class-name>
						<init-params>
							<init-param>
								<param-type>string</param-type>
								<param-value>config/client-pof-config.xml</param-value>
							</init-param>
						</init-params>
					</instance>
				</serializer>
			</initiator-config>
		</remote-cache-scheme>
	</caching-schemes>
</cache-config>